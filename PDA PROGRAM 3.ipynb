    "def count_characters(str):\n",
    " count={}\n",
    " for char in str:\n",
    "      if char in count:\n",
    "        count[char]+=1\n",
    "      else:\n",
    "        count[char]=1\n",
    " return count\n",
    "str=input(\"Enter a string  :  \")\n",
    "result=count_characters(str)\n",
    "for key,value in result.items():\n",
    "   print(f\"{key}:{value}\")"
   ]
